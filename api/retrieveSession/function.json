{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "sessionData",
      "databaseName": "draw",
      "collectionName": "draw",
      "connectionStringSetting": "cosmosConnectionString",
      "sqlQuery": "select * from c where c.sessionId = {sessionId}"
    }
  ]
}
