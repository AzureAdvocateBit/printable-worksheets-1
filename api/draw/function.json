{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "sessions/{session}/draw"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "signalR",
      "direction": "out",
      "name": "$return",
      "hubName": "{session}",
      "connectionStringSetting": "AzureSignalRConnectionString"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "cosmosDocument",
      "databaseName": "draw",
      "collectionName": "draw",
      "createIfNotExists": "true",
      "connectionStringSetting": "CosmosConnectionString",
      "partitionKey": "/id",
      "collectionThroughput": "400"
    }
  ]
}
